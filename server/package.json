  "bugs": {
    "url": "https://github.com/vseppala/deejii/issues"
  },
  "homepage": "https://github.com/vseppala/deejii#readme",
  "devDependencies": {
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "@types/debug": "^0.0.29",
    "@types/express": "^4.0.34",
    "@types/node": "^6.0.52"
  }
}
